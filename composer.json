{
    "name": "zendframework/zend-json-server",
    "description": "Zend Json-Server is a JSON-RPC server implementation.",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "json",
        "server",
        "json-server"
    ],
    "homepage": "https://github.com/zendframework/zend-json-server",
    "extra": {
        "branch-alias": {
            "dev-master": "2.6-dev",
            "dev-develop": "2.7-dev"
        }
    },
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-json": "dev-feature/split-zend-json-server",
        "zendframework/zend-http": "^2.5.4",
        "zendframework/zend-server": "^2.6.1"
    },
    "require-dev": {
        "zendframework/zend-stdlib": "^2.5 || ^3.0",
        "zendframework/zendxml": "^1.0.2",
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "~4.0"
    },
    "suggest": {
        "zendframework/zend-stdlib": "Zend\\Stdlib component, for use with caching Zend\\Json\\Server responses",
        "zendframework/zendxml": "To support Zend\\Json\\Json::fromXml() usage"
    },
    "autoload": {
        "psr-4": {
            "Zend\\Json\\Server\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZendTest\\Json\\Server\\": "test/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webit4me/zend-json"
        }
    ]
}
